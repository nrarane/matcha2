
<script>
    $( function() {
        $( "#slider-range" ).slider({
            range: true,
            min: 18,
            max: 55,
            values: [ 18, 33 ],
            slide: function( event, ui ) {
                $( "#age" ).val( ui.values[ 0 ] + " - " + ui.values[ 1 ] );
            }
        });
        $( "#age" ).val(  $( "#slider-range" ).slider( "values", 0 ) +
        " - " + $( "#slider-range" ).slider( "values", 1 ) );
    });
    $( function() {
        $( "#distanceR").slider({
            range: true,
            min: 1, 
            max: 30,
            values: [ 0, 30 ],
            slide: function( event, ui ) {
                $( "#distance").val(ui.values[ 0 ] + "kms - " + ui.values[ 1 ] + "kms" );
            }
        });
        $( "#distance").val( $( "#distanceR").slider( "values", 0 ) +
        "kms - " + $( "#distanceR").slider( "values", 1) + "kms" );
    });
</script>
